00042                 {\cf19 case} {\cf23 '?'}:\par
00043                     LOG(INFO) << {\cf22 "Unknown option given"};\par
00044                     std::cout << {\cf22 "Not know\\n"};\par
00045                     {\cf19 break};\par
00046 \par
00047                 {\cf19 case} {\cf23 'h'}:\par
00048                     LOG(INFO) << {\cf22 "Help option given"};\par
00049                     std::cout << {\cf22 "long h\\n"};\par
00050                     {\cf19 break};\par
00051 \par
00052                 {\cf19 case} {\cf23 'V'}:\par
00053                     LOG(INFO) << {\cf22 "Version option given"};\par
00054                     std::cout << {\cf22 "long V\\n"};\par
00055 \par
00056                 {\cf19 case} {\cf23 '0'}:\par
00057                     opt = std::make_unique<struct option>(long_options[optIndex]);\par
00058                     LOG(INFO) << {\cf22 "Option "} << opt->name << {\cf22 " given"};\par
00059 \par
00060                     {\cf19 if} (opt->has_arg == required_argument) \{\par
00061                         LOG(INFO) << {\cf22 "Argument: "} << optarg;\par
00062                     \}\par
00063                     {\cf19 break};\par
00064                 {\cf19 default}:\par
00065                     std::cout << {\cf22 "I shouldnt have been here!\\n"};\par
00066                     {\cf19 break};\par
00067             \}\par
00068         \} {\cf19 while} ({\cf17 true});\par
00069 \par
00070         LOG(INFO) << {\cf22 "Parsing options done"};\par
00071         std::optional<std::string> filename = \{\};\par
00072         LOG(INFO) << {\cf22 "Parsing other arguments..."};\par
00073 \par
00074         {\cf19 while} (optind < argc) \{\par
00075             {\cf19 if} (filename.has_value()) \{\par
00076                 LOG(ERROR) << {\cf22 "Only one filename can be given!"};\par
00077                 {\cf19 throw} std::invalid_argument({\cf22 "Only one filename can be given!\\n"});\par
00078             \}\par
00079 \par
00080             LOG(INFO) << {\cf22 "Filename set to: "} << argv[optind];\par
00081             filename = std::string(argv[optind++]);\par
00082         \}\par
00083 \par
00084         {\cf19 return} filename;\par
00085     \}\par
00086 \} {\cf20 // namespace utils}\par
}

